om-color: initial; }

.lensdb td { font-size: 1.2rem; vertical-align: top; padding: 10px; text-al=
ign: center; border: 1px solid rgb(105, 105, 105); background: none; }

.lensdb td:hover { background: none; }

.db_container { font-size: 1.4rem; width: 100%; margin: 0px; }

.db_container p { margin: 0px; }

.db_img { float: left; }

.db_info { padding-top: 15px; float: right; }

.db_details { width: 100%; overflow: hidden; padding-top: 30px; }

.db_details p { margin-bottom: 20px; }

